{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/TravelPin/browser",
  "installCommand": "npm install",
  "framework": "angular",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "${BACKEND_URL}/api/:path*"
    }
  ],
  "env": {
    "BACKEND_URL": "@backend_url"
  }
}
